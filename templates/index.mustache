<!DOCTYPE html>
<html>
<head>
    <title>{{siteConfig.title}}</title>
    {{>partials/head}}
</head>
<body>

<div>
    <span>{{siteConfig.title}}</span>
    {{#categories}}
        <span><a href="/category/{{alias}}">{{name}}</a></span>
    {{/categories}}
    <span><a href="/feed.xml">[feed]</a></span>
</div>
<div>
    <ul class="index_articles">
        {{#articles}}
            <li>
                <a href="/posts/{{alias}}">
                    {{title}}
                </a>
            </li>
        {{/articles}}
    </ul>
</div>
</body>
{{>partials/google_analytics}}
</html>
